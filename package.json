{
    "name": "snapsvg",
    "version": "1.0,1",
    "description": "JavaScript Vector Library",
    "main": "./dist/snap.svg.js",
    "module": "./dist/snap.esm.js",
    "exports": {
        ".": {
            "require": "./dist/snap.svg.js",
            "import": "./dist/snap.esm.mjs",
            "default": "./dist/snap.svg.js"
        },
        "./eve": {
            "require": "./dist/eve.cjs.js",
            "import": "./dist/eve.esm.mjs",
            "default": "./dist/eve.cjs.js"
        },
        "./mina": {
            "require": "./dist/mina.cjs.js",
            "import": "./dist/mina.esm.mjs",
            "default": "./dist/mina.cjs.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:adobe-webplatform/Snap.svg.git"
    },
    "author": "Dmitry Baranovskiy",
    "license": "Apache-2.0",
    "dependencies": {
        "eve": "^0.5.1",
        "grunt-contrib-uglify-es": "^3.3.0"
    },
    "devDependencies": {
        "docdash": "^2.0.2",
        "eslint": "^8.4.1",
        "expect.js": "*",
        "grunt": "1.5.3",
        "grunt-contrib-concat": "^2.1.0",
        "grunt-exec": "^0.4.7",
        "grunt-jsdoc": "^2.4.1",
        "grunt-prettify": "^0.4.0",
        "grunt-terser": "^2.0.0",
        "mocha": "*",
        "typescript": "^5.9.3"
    },
    "eslintConfig": {
        "globals": {
            "window": true,
            "console": true
        },
        "rules": {
            "block-scoped-var": 0,
            "comma-dangle": 0,
            "no-extra-parens": 1,
            "no-shadow": 0,
            "consistent-return": 0,
            "eqeqeq": 0,
            "no-new-wrappers": 0,
            "no-sequences": 1,
            "radix": 2,
            "new-parens": 0,
            "no-underscore-dangle": 0,
            "no-path-concat": 0,
            "strict": 0,
            "camelcase": 0,
            "no-extend-native": 0,
            "no-loop-func": 0,
            "new-cap": 0,
            "no-unused-expressions": 0,
            "no-mixed-requires": 0,
            "quotes": [
                1,
                "double",
                "avoid-escape"
            ]
        }
    }
}
